-- Scripts:

local function JIVUOHV_fake_script() -- Execute.ExecuteScript 
	local script = Instance.new('Script', Execute)

	function Execute()
		game.ReplicatedStorage.LynxEvent:FireServer(script.Parent.Parent.ModuleBox.Text)
	end
	
	script.Parent.MouseButton1Click:Connect(Execute)
end
coroutine.wrap(JIVUOHV_fake_script)()
local function RRZMH_fake_script() -- Clear.ClearScript 
	local script = Instance.new('Script', Clear)

	function Clear()
		script.Parent.Parent.ModuleBox.Text = ""
	end
	
	script.Parent.MouseButton1Click:Connect(Clear)
end
coroutine.wrap(RRZMH_fake_script)()
local function EJYGVVL_fake_script() -- Main.DragScript 
	local script = Instance.new('Script', Main)

	local pr = script.Parent
	pr.Active = true
	pr.Selectable = true
	pr.Draggable = true
end
coroutine.wrap(EJYGVVL_fake_script)()
